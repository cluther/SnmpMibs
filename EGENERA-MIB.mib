-- cBlade Virtual Ethernet table
-- Contains information about each network interface on the cBlade.


InterfaceOperStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
            "Describes network interface operational status."
    SYNTAX INTEGER {
        up(1),        -- ready to pass packets
        down(2),
        testing(3),   -- in some test mode
        unknown(4),   -- status can not be determined
                      -- for some reason.
        dormant(5),
        notPresent(6),    -- some component is missing
        lowerLayerDown(7) -- down due to state of
                          -- lower-layer interface(s)
    }

EthRethStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
            "Describes the state of the eth with respect to REth
            membership."
    SYNTAX INTEGER {
        active(1),      -- actively communcating
        standby(2),     -- waiting to take over in case of failure
        failed(3),      -- link or ping failure detected
        unconfigured(4),-- not part of a Reth
        unknown(999)
    }

